<!doctype html>
<!--[if IE 8 ]><html class="ie ie8" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><html lang="en" class="no-js"> <![endif]-->
<html lang="en">
<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
<head>

    <!-- Basic -->
    <title>@ViewBag.Title</title>

    <!-- Define Charset -->
    <meta charset="utf-8">
    <!-- Responsive Metatag -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <!-- Page Description and Author -->
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Content/images/apcrshr.ico">

    <!-- Responsive Metatag -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Page Description and Author -->
    <meta name="description" content="html">
    <meta name="author" content="DesignTEAM">


    <!-- Bootstrap CSS  -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.css" type="text/css" media="screen">

    <!-- Revolution Banner CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/settings.css" media="screen" />

    <!-- Vella CSS Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/style.css" media="screen">

    <!-- Responsive CSS Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/responsive.css" media="screen">

    <!-- Css3 Transitions Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/animate.css" media="screen">

    <!-- Color CSS Styles  -->
    @Styles.Render("~/bundles/css/colors")

    <!-- Fontello Icons CSS Styles  -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/fontello.css" media="screen">
    <!--[if IE 7]><link rel="stylesheet" href="~/Content/css/fontello-ie7.css"><![endif]-->
    <!-- Vella JS  -->

    @Scripts.Render("~/bundles/javascript");

    <!--[if IE 8]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <!--[if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->

</head>
<body>
    <!-- Container -->
    <div id="container">

        <!-- Start Header -->
        @Html.Partial("_NatigationBarPartialPage")
        <!-- End Header -->

        <!-- Start Page Banner -->
        <div class="page-banner no-subtitle">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <h2>@ViewBag.Title</h2>
                    </div>
                    <div class="col-md-6">
                        <ul class="breadcrumbs">
                            <li><a href="/">Home</a></li>
                            <li>@ViewBag.Title</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Page Banner -->

        <!-- Start Content -->
        <div id="content">
            <div class="container">
                <div class="row sidebar-page">


                    <!-- Page Content -->
                    @RenderBody()
                    <!-- End Page Content-->

                    <!--Sidebar-->
                    <div class="col-md-3 sidebar right-sidebar">

                        <!-- Search Widget -->
                        <div class="widget widget-search">
                            <form action="#">
                                <input type="search" placeholder="Enter Keywords..." />
                                <button class="search-btn" type="submit"><i class="icon-search-1"></i></button>
                            </form>
                        </div>
                        <!-- Categories Widget -->
                        <div class="widget widget-categories">
                            <h4>Categories <span class="head-line"></span></h4>
                            <ul>
                                <li>
                                    <a class='@(ViewBag.CurrentNode == "Delegate" ? "active" : "")' href='@Url.Action("Delegate", "Home")'>Delegate Information</a>
                                </li>
                                <li>
                                    <a class='@(ViewBag.CurrentNode == "Instruction" ? "active" : "")' href='@Url.Action("Instruction", "Home")'>Instruction</a>
                                </li>
                                <li>
                                    <a class='@(ViewBag.CurrentNode == "Donnor" ? "active" : "")' href='@Url.Action("Donnor", "Home")'>Donnor Information</a>
                                </li>
                                <li>
                                    <a class='@(ViewBag.CurrentNode == "Location" ? "active" : "")' href='@Url.Action("Contact", "Home")'>Location</a>
                                </li>
                                <li>
                                    <a class='@(ViewBag.CurrentNode == "Travel" ? "active" : "")' href='@Url.Action("Travel", "Home")'>Travel and Accommodation</a>
                                </li>
                                <li>
                                    <a class='@(ViewBag.CurrentNode == "Presentation" ? "active" : "")' href='@Url.Action("PresentationData", "Home")'>Presentation Data</a>
                                </li>
                            </ul>
                        </div>

                        <!-- Video Widget -->
                        <div class="widget">
                            <h4>Video <span class="head-line"></span></h4>
                            <div>
                                <iframe src="http://player.vimeo.com/video/74013667?byline=0&amp;portrait=0&amp;badge=0" width="800" height="450" frameborder="0"></iframe>
                            </div>
                        </div>

                        <!-- Popular Posts widget -->
                        <div class="widget widget-popular-posts">
                            <h4>Like Us on Facebook<span class="head-line"></span></h4>
                            <iframe src="//www.facebook.com/plugins/like.php?href=https%3A%2F%2Fwww.facebook.com%2FVietBook.com.vn&amp;width=194&amp;layout=standard&amp;action=like&amp;show_faces=true&amp;share=true&amp;height=90" scrolling="no" frameborder="0" style="border:none; overflow:hidden; width:100%; height:100%;" allowtransparency="true"></iframe>
                        </div>
                    
                    </div>
                    <!--End sidebar-->


                </div>
            </div>
        </div>
        <!-- End Content -->

        <!-- Start Footer -->
        @Html.Partial("_FooterPartialPage")
        <!-- End Footer -->

    </div>
    <!-- End Container -->
    <!-- Go To Top Link -->
    <a href="#" class="back-to-top"><i class="icon-up-open-1"></i></a>

    <div id="loader">
        <div class="spinner">
            <div class="dot1"></div>
            <div class="dot2"></div>
        </div>
    </div>

    <script type="text/javascript">
        $(".search-side").hide();
    </script>
</body>
</html>