@using apcrshr_site.Helper
@model Site.Core.DataModel.Model.PhotoModel
@{
    ViewBag.Title = "Thêm Ảnh";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
    SelectList albums = ViewBag.Albums;
}

<div class="row">
    <div class="col-sm-12">
        <div class="box">
            <div class="box-title">
                <h3>
                    <i class="fa fa-th"></i>Multi File upload
                </h3>
            </div>
            <div class="box-content nopadding">
                <div class="plupload"></div>
            </div>
        </div>
    </div>
    
    <div class="col-sm-12">
        <div class="box">
            <div class="box-title">
                <h3>
                    <i class="fa fa-th"></i>Tùy chọn 
                </h3>
            </div>
            <div class="box-content">
                <a href='@Url.Action("Index")' class="btn btn-info">Hủy Bỏ</a>
            </div>
        </div>
    </div>
</div>

